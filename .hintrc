{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]",
          "img[loading]"
        ]
      }
    ],
    "axe/text-alternatives": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "scroll-behavior",
          "overscroll-behavior-inline",
          "scroll-padding-inline",
          "overscroll-behavior",
          "overscroll-behavior: contain",
          "overscroll-behavior-inline: contain"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "button-type": "off",
    "no-inline-styles": "off"
  }
}